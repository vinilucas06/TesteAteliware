@model PortalTest.Models.GitRepositorie
@{
    ViewData["Title"] = "Detalhe";
}

<h3>Detalhe Repositório</h3>

<div class="row">
    <div class="col-sm-4">Nome</div>
    <div class="col-sm-8">@Model.Name</div>
</div>
<div class="row">
    <div class="col-sm-4">Linguagem</div>
    <div class="col-sm-8">@Model.Language</div>
</div>
<div class="row">
    <div class="col-sm-4">Url</div>
    <div class="col-sm-8">@Model.Url</div>
</div>
<div class="row">
    <div class="col-sm-4">Último Update</div>
    <div class="col-sm-8">@Model.LastUpdated</div>
</div>
<h3>Descrição</h3>
@Model.Description

